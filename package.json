    "install-bot": "node lib/install.js"
  },
  "dependencies": {
    "chai": "^4.0.2",
    "email-validator": "^1.0.7",
    "express": "4.10.2",
    "handlebars": "^4.0.10",
    "jsforce": "^1.7.1",
    "lodash": "^4.17.4",
    "mocha": "^3.4.2",
    "moment": "^2.18.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-html-parser": "^1.0.3",
    "sendgrid": "^5.1.2",
    "sinon": "^2.3.7",
    "socket.io": "1.2.0",
    "socket.io-client": "^2.0.3",
    "superagent": "^3.6.0",
    "react-select": "^1.0.1"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "es2015",
      "stage-2"
