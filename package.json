  "description": "A bot for PagerDuty integration",
  "url": "https://git.soma.salesforce.com/Refocus-Bots/Events-Bot",
  "scripts": {
    "start-ui": "webpack-dev-server --open",
    "start": "npm run build-ui && npm run install-bot && node index.js",
